        "Hostnames": [
          "localhost:60002",
          "localhost:44015"
        ],
        "Theme": "Darkly",
        "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=saaskit-sample-tenant2;Trusted_Connection=True;MultipleActiveResultSets=true"
      }
    ]
  }
}
